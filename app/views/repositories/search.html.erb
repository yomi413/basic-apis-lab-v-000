<h1>Search Github Repositories</h1>
<%= form_tag '/search' do %>
  <%= label_tag :query %>
  <%= text_field_tag :query %>
  <%= submit_tag "Search Github!" %>
<% end %>

<div>
  <% if @items %>
    <ul>
    <% @items.each do |item| %>
      <li>
        <%= binding.pry %>
        <%= item["html_url"] %>
      </li>
    <% end %>
    </ul>
  <% end %>
</div>
